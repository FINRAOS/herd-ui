    "puppeteer": "1.8.0",
    "q": "1.5.1",
    "secretlint": "^2.1.1",
    "sync-request": "6.0.0",
    "then-request": "6.0.0",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "typescript": "3.0.3",
    "var": "0.3.2",
    "webdriver-manager": "12.1.0",
    "winston": "3.1.0"
  },
  "pre-commit": [
    "license-check",
    "lint"
  ]
}
